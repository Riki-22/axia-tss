# C:\trading_system/src/ec2/data_collector\.env (On EC2 Instance)

# AWS Region for Boto3 clients
AWS_REGION="ap-northeast-1"
# Name of the secret in AWS Secrets Manager containing MT5 credentials
MT5_SECRET_NAME_DC="mt5/axiory/demo-credentials"
# Full path to the MT5 terminal.exe on the EC2 instance
# Example for Windows: C:\Program Files\MetaTrader 5\terminal64.exe
MT5_TERMINAL_PATH_DC="C:\Program Files\Axiory MetaTrader 5\terminal64.exe"
# S3 bucket name for storing raw data
S3_RAW_DATA_BUCKET="tss-raw-data"
# Comma-separated list of symbols to collect data for
DATA_COLLECTION_SYMBOLS="USDJPY,EURUSD,EURJPY,GBPUSD,GBPJPY,EURGBP"
# Comma-separated list of timeframes (M1, M5, M15, M30, H1, H4, D1, W1, MN1)
DATA_COLLECTION_TIMEFRAMES="M1,M5,M15,M30,H1,H4,D1,W1,MN1"
# Number of bars/candles to fetch in each collection cycle
DATA_FETCH_COUNTS_JSON = '{"M1": 1440, "M5": 1000,"M15": 1000,"M30": 480,"H1": 240,"H4": 60,"D1": 10,"W1": 2,"MN1": 1,"DEFAULT": 1440}'